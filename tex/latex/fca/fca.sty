\newcommand\fca@@labelBox[1]{%
  \begin{pgfscope}%
    \pgfqkeys{/fca}{every node/.try,every label/.try,#1}%
    \fca@labelslayer
    \pgfkeysvalueof{/fca/node font}%
    \fca@options
    \pgftransformshift{%
      \pgfpointanchor{%
        \pgfkeysvalueof{/fca/pgfnode}%
      }{\fca@label@at}%
    }%
    \fca@parselength\@tempdima{\fca@label@shift@x}\unitlength
    \fca@parselength\@tempdimb{\fca@label@shift@y}\unitlength
    \pgftransformshift{\pgfpoint{\@tempdima}{\@tempdimb}}%
    \fca@transform
    \pgfnode{rectangle}{\fca@label@anchor}{%
      \fca@typeset@node@content
      {%
        \pgfkeysvalueof{/fca/node contents}%
      }%
    }{%
      \fca@label@name@prefix\pgfkeysvalueof{/fca/node}%
      \fca@label@name@suffix
    }{%
      \expandafter\pgfusepath\expandafter{\fca@usepath}%
    }%
    \fca@endlabelslayer
    % pin edge
    \pgftransformreset
    \let\fca@options\fca@defaultoptions
    \pgfqkeys{/fca}{#1}%
    \pgfqkeys{/fca}{every label edge/.try}%
    \fca@options
    \fca@transform
    \pgfsetlinewidth{\fca@edge@thickness}%
    \fca@prepare@node\fca@temp@start@point\fca@temp@start@point@{}{}{\pgfkeysvalueof{/fca/pgfnode}}%
    \fca@prepare@node\fca@temp@end@point\fca@temp@end@point@
    \fca@label@name@prefix\fca@label@name@suffix
    {\pgfkeysvalueof{/fca/node}.\fca@label@anchor}%
    \pgfpathmoveto{\fca@temp@start@point\fca@temp@end@point@}%
    \pgfpathlineto{\fca@temp@end@point\fca@temp@start@point@}%
    \pgfusepath{stroke}%
    % overlay concept
    \pgftransformreset
    \let\fca@options\fca@defaultoptions
    \pgfsetdash{}{0pt}
    \let\fca@shape\pgfkeysnovalue
    \pgfqkeys{/fca}{#1}%
    \pgfqkeys{/fca}{every label concept/.try}%
    \pgfqkeys{/fca}{label concept/.try}%
    \ifx\fca@shape\pgfkeysnovalue
    \else
      \pgftransformshift{%
        \pgfpointanchor{%
          \pgfkeysvalueof{/fca/pgfnode}%
        }{\fca@label@at}%
      }%
      \pgfusepath{stroke}%
      \fca@transform
      \fca@options
      % \pgfsetstrokecolor{\fca@node@color}%
      \pgfnode{\fca@shape}{\fca@label@anchor}{}{%
        \fca@label@name@prefix\pgfkeysvalueof{/fca/node}%
        \fca@label@name@suffix
      }{\expandafter\pgfusepath\expandafter{\fca@usepath}}%
    \fi
  \end{pgfscope}%
}
